schema{
 query: Query
 mutation : Mutation
}

scalar DateTime

type Query {
	findAllHistory(email: String): Response_History
}

type Mutation {
	addHistory(history: [HistoryInput]): BaseHistory
	clearHistory(email: String, type: String): [BaseHistory]
}

type BaseHistory {
	id: ID,
	email: String,
	title: String,
	type: String
}

type Response_History {
	id: ID,
	historyArr: [BaseHistory]
}

input HistoryInput {
	id: ID,
	email: String,
	title: String,
	type: String,	
	
}

