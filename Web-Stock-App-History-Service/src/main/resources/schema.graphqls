schema{
 query: Query
 mutation : Mutation
}

scalar DateTime

type Query {
	findAllHistory(email: String): [BaseHistory]
}

type Mutation {
	addHistory(email: String, title: String, type: String): BaseHistory
	clearHistory(email: String, type: String): [BaseHistory]
}

type BaseHistory {
	id: ID,
	email: String,
	title: String,
	type: String
}

input HistoryInput {
	id: ID,
	email: String,
	title: String,
	type: String,
}

